# Base API URL (preferred)
REACT_APP_API_BASE=http://localhost:8000

# Fallbacks
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=

# Optional healthcheck path. Use 'none' to disable.
REACT_APP_HEALTHCHECK_PATH=/health

# Feature flags: JSON or comma-separated list
REACT_APP_FEATURE_FLAGS={"showFooter":true}

# Additional optional variables
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_EXPERIMENTS_ENABLED=false
